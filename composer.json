{
  "name": "glagola/collection",
  "description": "A library provides immutable collection, which can tell which items was added or removed",
  "type": "library",
  "license":           "MIT",
  "homepage":          "https://github.com/glagola/collection",
  "authors": [
    {
      "name": "Igor Glagola",
      "email": "igor@glagola.ru"
    }
  ],

  "keywords": [
    "collection",
    "immutable",
    "diff"
  ],
  "require": {
